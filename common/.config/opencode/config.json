{
  "$schema": "https://opencode.ai/config.json",
  "plugin":
  [
    "opencode-openai-codex-auth@4.3.0",
    "@mohak34/opencode-notifier@0.1.10",
    "opencode-gemini-auth@1.3.6",
    "opencode-scheduler"
  ],
  "mcp": {
    "kagi-ken": {
      "type": "local",
      "command": ["npx", "-y", "github:czottmann/kagi-ken-mcp"],
      "environment": {
      },
      "enabled": true
    },
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
      },
      "enabled": true
    },
    "linear": {
      "type": "remote",
      "url": "https://mcp.linear.app/mcp",
      "enabled": true
    },
    "clickup": {
      "type": "local",
      "command": ["npx", "-y", "mcp-remote", "https://mcp.clickup.com/mcp"],
      "enabled": false
    }
  }
}
